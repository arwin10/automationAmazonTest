			Log.info("Type Dropdown is displayed.");
			System.out.println("Type Dropdown is displayed.");

		}
		else
		{
			Log.warn("Type Dropdown is not displayed.");
			System.out.println("Type Dropdown  is not displayed.");

		}

		return element;

	}
	
	public static WebElement drpdwn_TypeOption(WebDriver driver){

		element = driver.findElement(By.xpath("//li[@class='ui-selectonemenu-item ui-selectonemenu-list-item ui-corner-all' and text()='Manual Commission']"));

		if(element!=null)
		{   
			Log.info("Type dropdown option is displayed.");
			System.out.println("Type dropdown option is displayed.");

		}
		else
		{
			Log.warn("Type dropdown option is not displayed.");
			System.out.println("Type dropdown option is not displayed.");

		}

		return element;

	}
	
	public static WebElement drpdwn_Brand(WebDriver driver){

		element = driver.findElement(By.xpath("//div[@id='frm1:commissionBrand']"));

		if(element!=null)
		{   
			Log.info("Brand Dropdown is displayed.");
			System.out.println("Brand Dropdown is displayed.");

		}
		else
		{
			Log.warn("Brand Dropdown is not displayed.");
			System.out.println("Brand Dropdown  is not displayed.");

		}

		return element;

	}
	
	public static WebElement drpdwn_BrandOption(WebDriver driver){

		element = driver.findElement(By.xpath("//div[@id='frm1:commissionBrand_panel']/div/ul/li[@class='ui-selectonemenu-item ui-selectonemenu-list-item ui-corner-all' and text()='Apple']"));

		if(element!=null)
		{   
			Log.info("Brand dropdown option is displayed.");
			System.out.println("Brand dropdown option is displayed.");

		}
		else
		{
			Log.warn("Brand dropdown option is not displayed.");
			System.out.println("Brand dropdown option is not displayed.");

		}

		return element;

	}
