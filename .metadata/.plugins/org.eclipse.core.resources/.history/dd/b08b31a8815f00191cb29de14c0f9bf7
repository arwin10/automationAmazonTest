	public static WebElement header_Refferals(WebDriver driver){

		element = driver.findElement(By.xpath("//h4[@class='pull-left' and text()='Referrals']"));

		if(element!=null)
		{   
			Log.info("Referrals header is displayed.");
			System.out.println("Referrals header is displayed.");

		}
		else
		{
			Log.warn("Referrals header is not displayed.");
			System.out.println("Referrals header is not displayed.");

		}

		return element;

	}
	
	public static WebElement div_RelatedFilter(WebDriver driver){

		element = driver.findElement(By.xpath("//span[@class='select2-choice-type' and text()='Related']"));

		if(element!=null)
		{   
			Log.info("Related Filter is displayed.");
			System.out.println("Related Filter is displayed.");

		}
		else
		{
			Log.warn("Related Filter is not displayed.");
			System.out.println("Related Filter is not displayed.");

		}

		return element;

	}
	
	public static WebElement div_RelatedFilterOption(WebDriver driver){

		element = driver.findElement(By.xpath("//div[@class='select2-result-label']/div[contains(text(),'All')]"));

		if(element!=null)
		{   
			Log.info("Referrals Option is displayed.");
			System.out.println("Referrals Option is displayed.");

		}
		else
		{
			Log.warn("Referrals Option is not displayed.");
			System.out.println("Referrals Option is not displayed.");

		}

		return element;

	}
	
}
